{
  "name": "lerna-changed",
  "version": "1.1.6",
  "private": true,
  "description": "Github action for determining the changed packages in a lerna monorepo",
  "main": "dist/lerna-changed.js",
  "scripts": {
    "build": "webpack --config ./webpack.config.js",
    "clean": "rimraf dist/",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./",
    "prebuild": "npm run clean",
    "release": "semantic-release",
    "start": "bin/lerna-changed",
    "test": "jest --config=jest.config.js --silent"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/allenevans/lerna-changed.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Allen Evans",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.2.6",
    "@lerna/collect-updates": "3.20.0",
    "@lerna/package-graph": "3.18.5",
    "@lerna/project": "4.0.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/npm": "7.0.9",
    "@semantic-release/release-notes-generator": "9.0.1",
    "@types/jest": "26.0.19",
    "@types/node": "14.14.12",
    "eslint": "7.32.0",
    "eslint-config-airbnb-typescript-prettier": "4.2.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "26.6.3",
    "lerna": "3.22.1",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "semantic-release": "17.3.0",
    "ts-jest": "26.4.4",
    "ts-loader": "8.0.12",
    "ts-node": "9.1.1",
    "typescript": "4.1.2",
    "webpack": "5.10.0",
    "webpack-cli": "4.2.0"
  }
}
