{
  "name": "lerna-changed",
  "version": "2.0.0",
  "private": true,
  "description": "Github action for determining the changed packages in a lerna monorepo",
  "main": "dist/lerna-changed.js",
  "scripts": {
    "build": "webpack --config ./webpack.config.js",
    "clean": "rimraf dist/",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./",
    "prebuild": "npm run clean",
    "release": "semantic-release",
    "start": "bin/lerna-changed",
    "test": "jest --config=jest.config.js --silent"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/allenevans/lerna-changed.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Allen Evans",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.10.0",
    "@lerna/collect-updates": "6.0.3",
    "@lerna/package-graph": "6.0.3",
    "@lerna/project": "6.0.3"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "9.0.1",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@types/jest": "29.2.2",
    "@types/node": "18.11.9",
    "eslint": "8.27.0",
    "eslint-config-airbnb-typescript-prettier": "5.0.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.3.1",
    "lerna": "6.0.3",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "semantic-release": "19.0.5",
    "ts-jest": "29.0.3",
    "ts-loader": "9.4.1",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "webpack": "5.75.0",
    "webpack-cli": "4.10.0"
  }
}
