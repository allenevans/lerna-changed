{
  "name": "lerna-changed",
  "version": "1.1.0",
  "private": true,
  "description": "Github action for determining the changed packages in a lerna monorepo",
  "main": "dist/lerna-changed.js",
  "scripts": {
    "build": "webpack --config ./webpack.config.js",
    "clean": "rimraf dist/",
    "lint": "tslint -c tslint.json --project tsconfig.json",
    "prebuild": "npm run clean",
    "release": "semantic-release",
    "start": "bin/lerna-changed",
    "test": "jest --config=jest.config.js --silent"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/allenevans/lerna-changed.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Allen Evans",
  "license": "MIT",
  "devDependencies": {
    "@actions/core": "1.2.0",
    "@semantic-release/changelog": "3.0.6",
    "@semantic-release/commit-analyzer": "6.3.3",
    "@semantic-release/git": "7.0.18",
    "@semantic-release/npm": "5.3.5",
    "@semantic-release/release-notes-generator": "7.3.5",
    "@types/jest": "24.0.25",
    "@types/node": "13.1.1",
    "jest": "24.9.0",
    "lerna": "3.20.1",
    "prettier": "1.19.1",
    "rimraf": "3.0.0",
    "semantic-release": "15.14.0",
    "ts-jest": "24.2.0",
    "ts-loader": "6.2.1",
    "ts-node": "8.5.4",
    "tslint": "5.20.1",
    "tslint-config-airbnb": "5.11.2",
    "typescript": "3.7.4",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10"
  },
  "peerDependencies": {
    "@lerna/collect-updates": "3.16.0",
    "@lerna/package-graph": "3.16.0",
    "@lerna/project": "3.16.0"
  }
}
