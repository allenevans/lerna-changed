{
  "name": "lerna-changed",
  "version": "1.3.0",
  "private": true,
  "description": "Github action for determining the changed packages in a lerna monorepo",
  "main": "dist/lerna-changed.js",
  "scripts": {
    "build": "webpack --config ./webpack.config.js",
    "clean": "rimraf dist/",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./",
    "prebuild": "npm run clean",
    "release": "semantic-release",
    "start": "bin/lerna-changed",
    "test": "jest --config=jest.config.js --silent"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/allenevans/lerna-changed.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Allen Evans",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.6.0",
    "@lerna/collect-updates": "3.20.0",
    "@lerna/package-graph": "3.18.5",
    "@lerna/project": "3.21.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "8.0.3",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@types/jest": "27.0.3",
    "@types/node": "17.0.1",
    "eslint": "8.5.0",
    "eslint-config-airbnb-typescript-prettier": "5.0.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.4.5",
    "lerna": "3.22.1",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "semantic-release": "18.0.1",
    "ts-jest": "27.1.2",
    "ts-loader": "9.2.6",
    "ts-node": "10.4.0",
    "typescript": "4.5.4",
    "webpack": "5.65.0",
    "webpack-cli": "4.9.1"
  }
}
